@model IEnumerable<TipoCabanaModel>

@{
    ViewData["Title"] = "ListarTipoCabana";
}

<h1>ListarTipoCabana</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CostoPorHuesped)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoCabana)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CostoPorHuesped)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoCabana)
            </td>
            <td>

            </td>
        </tr>
}
    </tbody>
</table>
